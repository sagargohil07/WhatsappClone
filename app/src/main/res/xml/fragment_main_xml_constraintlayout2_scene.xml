<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="200">
        <KeyFrameSet></KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start"></ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/floatAddStatus"
            android:layout_width="@dimen/dimen_35"
            android:layout_height="@dimen/dimen_35"
            android:layout_marginBottom="@dimen/dimen_18"
            motion:layout_constraintBottom_toTopOf="@id/floatButton"
            motion:layout_constraintEnd_toEndOf="@id/floatButton"
            motion:layout_constraintStart_toStartOf="@id/floatButton" />
    </ConstraintSet>
</MotionScene>